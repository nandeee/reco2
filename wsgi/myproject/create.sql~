create table item ("item_id" integer(2) NOT NULL primary key autoincrement,color varchar(10) NOT NULL,size char NOT NULL,typ varchar(15),fabric integer(2),styling varchar(30),brand varchar(15));

create table bstructure(typ integer(2) primary key autoincrement,complexion char,fav_color varchar(10) NOT NULL,fav_type varchar(10) NOT NULL,fav_style varchar(30) NOT NULL);

 